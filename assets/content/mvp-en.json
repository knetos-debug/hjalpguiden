      "id": "trygghet-fraser",
      "module": "Trygghet",
      "title": {
        "sv_enkel": "",
        "hs": "Safety - Important phrases to show"
      },
      "prereq": [
        {
          "sv_enkel": "",
          "hs": "Have phone ready to show."
        }
      ],
      "steps": [
        {
          "sv_enkel": "",
          "hs": "Phrase 1: Speak slowly. I use translation."
        },
        {
          "sv_enkel": "",
          "hs": "Phrase 2: Can you write it? I understand better."
        },
