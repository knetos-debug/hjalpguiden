        {
          "sv_enkel": "",
          "hs": "Point camera at QR code. Hold still."
        },
        {
          "sv_enkel": "",
          "hs": "Tap the notification that appears."
        },
        {
          "sv_enkel": "",
          "hs": "Link opens in browser."
        }
      ],
      "troubleshoot": [
        {
          "sv_enkel": "",
          "hs": "QR code doesn't work → Use Google Lens instead.",
          "stepIndex": 4
        },
        {
          "sv_enkel": "",
          "hs": "Link doesn't open → Tap and hold, choose Open.",
          "stepIndex": 1
        }
      ],
      "sources": [],
      "lastVerified": null
    },
    {
      "id": "mobil-skarmdump",
      "module": "Mobil",
      "title": {
        "sv_enkel": "",
        "hs": "Phone Basics - Take screenshot"
      },
      "prereq": [
        {
          "sv_enkel": "",
          "hs": "Have the screen you want to capture open."
        }
      ],
      "steps": [
        {
          "sv_enkel": "",
          "hs": "Android: Hold down Power + Volume down together."
        },
        {
          "sv_enkel": "",
          "hs": "iPhone: Press Side + Volume up together."
        },
        {
          "sv_enkel": "",
          "hs": "You'll see a flash. Image is saved."
        },
        {
          "sv_enkel": "",
          "hs": "Open Gallery or Photos to see the image."
        },
        {
          "sv_enkel": "",
          "hs": "Tap Share to send the image."
        },
        {
          "sv_enkel": "",
          "hs": "Choose how to share: SMS, email, WhatsApp."
        }
      ],
      "troubleshoot": [
        {
          "sv_enkel": "",
          "hs": "Can't find image → Look in Screenshots folder.",
          "stepIndex": 4
        },
        {
          "sv_enkel": "",
          "hs": "Doesn't work → Try holding buttons longer.",
          "stepIndex": 1
        }
      ],
      "sources": [],
      "lastVerified": null
