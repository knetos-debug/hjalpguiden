      "id": "bankid-login-same",
      "module": "BankID",
      "title": {
        "sv_enkel": "",
        "hs": "BankID - Log in (same device)"
      },
      "prereq": [
        {
          "sv_enkel": "",
          "hs": "Have BankID app installed."
        },
        {
          "sv_enkel": "",
          "hs": "Remember your BankID code."
        }
      ],
      "steps": [
        {
          "sv_enkel": "",
          "hs": "Go to website. Tap Log in."
        },
        {
          "sv_enkel": "",
