    {
      "id": "mobil-wifi",
      "module": "Mobil",
      "title": {
        "sv_enkel": "",
        "hs": "Phone Basics - Connect Wi-Fi"
      },
      "prereq": [
        {
          "sv_enkel": "",
          "hs": "Have your phone turned on."
        },
        {
          "sv_enkel": "",
          "hs": "Have the Wi-Fi password ready."
        }
      ],
      "steps": [
        {
          "sv_enkel": "",
          "hs": "Open Settings on the phone."
        },
        {
          "sv_enkel": "",
          "hs": "Tap Wi-Fi or Wireless network."
        },
        {
          "sv_enkel": "",
          "hs": "See the list of networks. Choose your network."
        },
        {
          "sv_enkel": "",
          "hs": "Enter the Wi-Fi password. Tap Connect."
        },
        {
          "sv_enkel": "",
          "hs": "Wait. You'll see a checkmark when done."
        },
        {
          "sv_enkel": "",
          "hs": "You are now connected to Wi-Fi."
        }
      ],
      "troubleshoot": [
        {
          "sv_enkel": "",
          "hs": "Can't find network → Move closer to the router.",
          "stepIndex": 3
        },
        {
          "sv_enkel": "",
          "hs": "Wrong password → Double-check capital/lowercase letters.",
          "stepIndex": 4
        }
      ],
      "sources": [],
      "lastVerified": null
    },
    {
      "id": "mobil-oppna-lank",
      "module": "Mobil",
      "title": {
        "sv_enkel": "",
        "hs": "Phone Basics - Open link & QR code"
      },
      "prereq": [
        {
          "sv_enkel": "",
          "hs": "Have a link or QR code to open."
        }
      ],
      "steps": [
        {
          "sv_enkel": "",
          "hs": "For link: Tap the link. It turns blue."
