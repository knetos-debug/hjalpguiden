      "id": "mobil-wifi",
      "module": "Mobil",
      "title": {
        "sv_enkel": "Mobilens grunder - Anslut Wi-Fi",
        "hs": "[HS]"
      },
      "prereq": [
        {
          "sv_enkel": "Ha din telefon påslagen.",
          "hs": "[HS]"
        },
        {
          "sv_enkel": "Ha Wi-Fi-lösenordet redo.",
          "hs": "[HS]"
        }
      ],
      "steps": [
        {
          "sv_enkel": "Öppna Inställningar på telefonen.",
          "hs": "[HS]"
        },
        {
          "sv_enkel": "Tryck på Wi-Fi eller Trådlöst nätverk.",
          "hs": "[HS]"
        },
        {
          "sv_enkel": "Se listan över nätverk. Välj ditt nätverk.",
          "hs": "[HS]"
        },
        {
          "sv_enkel": "Skriv Wi-Fi-lösenordet. Tryck Anslut.",
          "hs": "[HS]"
        },
        {
          "sv_enkel": "Vänta. Du ser ett kryss-tecken när klart.",
          "hs": "[HS]"
        },
        {
          "sv_enkel": "Du är nu ansluten till Wi-Fi.",
          "hs": "[HS]"
        }
      ],
      "troubleshoot": [
        {
          "sv_enkel": "Hittar inte nätverket → Gå närmare routern.",
          "hs": "[HS]",
          "stepIndex": 3
        },
        {
          "sv_enkel": "Fel lösenord → Dubbelkolla stora/små bokstäver.",
          "hs": "[HS]",
          "stepIndex": 4
        }
      ],
      "sources": [
        {
          "label": "Android Support - Wi-Fi",
          "url": "https://support.google.com/android/"
        }
      ],
      "lastVerified": "2025-10-24"
