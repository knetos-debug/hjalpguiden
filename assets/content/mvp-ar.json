    {
      "id": "1177-login-same",
      "module": "m1177",
      "title": "1177 – Logga in (samma enhet)",
      "prereq": [
        {
          "sv_enkel": "",
          "hs": "احصل على تطبيق بنك آي دي على هاتفك."
        },
        {
          "sv_enkel": "",
          "hs": "تذكر رمز بنك آي دي الخاص بك."
        }
      ],
      "steps": [
        {
          "sv_enkel": "",
          "hs": "افتح الموقع 1177.se في متصفحك."
        },
        {
          "sv_enkel": "",
          "hs": "اضغط على زر تسجيل الدخول."
        },
        {
          "sv_enkel": "",
          "hs": "اكتب رقمك الشخصي. اضغط على التالي."
        },
        {
          "sv_enkel": "",
          "hs": "يفتح بنك آي دي تلقائيًا. انتظر."
        },
        {
          "sv_enkel": "",
          "hs": "اكتب رمز بنك آي دي. اضغط موافق."
        },
        {
          "sv_enkel": "",
          "hs": "أنت الآن مسجل الدخول إلى 1177."
        }
      ],
      "troubleshoot": [
        {
          "sv_enkel": "",
          "hs": "لا يفتح بنك آي دي ← افتح تطبيق بنك آي دي يدويًا.",
          "stepIndex": 4
        },
        {
          "sv_enkel": "",
          "hs": "رمز خاطئ ← حاول مرة أخرى. بعد ثلاث محاولات يجب الانتظار.",
          "stepIndex": 5
        }
      ],
