    {
      "id": "1177-login-other",
      "module": "m1177",
      "title": "1177 – Logga in (annan enhet)",
      "prereq": [
        {
          "sv_enkel": "",
          "hs": "احصل على بنك آي دي على هاتفك."
        },
        {
          "sv_enkel": "",
          "hs": "يجب أن يكون الهاتف بالقرب منك."
        }
      ],
      "steps": [
        {
          "sv_enkel": "",
          "hs": "افتح الموقع 1177.se على الكمبيوتر."
        },
        {
          "sv_enkel": "",
          "hs": "اضغط تسجيل الدخول. اكتب الرقم الشخصي."
        },
        {
          "sv_enkel": "",
          "hs": "اختر بنك آي دي على جهاز آخر."
        },
        {
          "sv_enkel": "",
          "hs": "افتح بنك آي دي على الهاتف."
        },
        {
          "sv_enkel": "",
          "hs": "اضغط صادق على نفسك. اكتب رمزك."
        },
        {
          "sv_enkel": "",
          "hs": "أنت الآن مسجل الدخول على الكمبيوتر."
        }
      ],
      "troubleshoot": [
        {
          "sv_enkel": "",
          "hs": "لا يحدث شيء ← تحقق من أن الرقم الشخصي صحيح.",
          "stepIndex": 2
        },
        {
          "sv_enkel": "",
          "hs": "لا أرى شيئًا في بنك آي دي ← اضغط على النص صادق على نفسك.",
          "stepIndex": 4
        }
      ],
      "sources": [],
      "lastVerified": null
    },
    {
