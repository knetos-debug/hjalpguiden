      "id": "mobil-wifi",
      "module": "Mobil",
      "title": "Mobilens grunder - Anslut Wi-Fi",
      "prereq": [
        {
          "sv_enkel": "",
          "hs": "Telefonunuzun açık olduğundan emin olun."
        },
        {
          "sv_enkel": "",
          "hs": "Wi-Fi şifresini hazır bulundurun."
        }
      ],
      "steps": [
        {
          "sv_enkel": "",
          "hs": "Telefonunuzda Ayarlar uygulamasını açın."
